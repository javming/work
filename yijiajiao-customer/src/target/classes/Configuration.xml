<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias alias="Customer" type="com.yijiajiao.customer.model.Customer" />
		<typeAlias alias="Complain" type="com.yijiajiao.customer.model.Complain" />
		<typeAlias alias="Invoice" type="com.yijiajiao.customer.model.Invoice" />
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="com.mysql.jdbc.Driver" />
				<property name="url" value="jdbc:mysql://192.168.1.169:3306/yijiajiao" />
				<property name="username" value="root" />
				<property name="password" value="jiao" />

			</dataSource>
		</environment>
	</environments>


	<mappers>
		<mapper resource="com/yijiajiao/customer/model/Customer.xml" />
		<mapper resource="com/yijiajiao/customer/model/Complain.xml" />
		<mapper resource="com/yijiajiao/customer/model/Invoice.xml" />
	</mappers>

</configuration>